          "data_type": "continuous",
          "unit": "mm Hg",
          "range": [70, 250],
          "component": "Examination",
          "cycles": ["2015-2016", "2017-2018", "2019-2020"],
          "aliases": ["systolic", "sbp"]
        },
        "diastolic_bp": {
          "nhanes_code": "BPXDI1",
          "label": "Diastolic: Blood pres (1st rdg) mm Hg",
          "description": "Diastolic blood pressure (first reading)",
          "data_type": "continuous",
          "unit": "mm Hg",
          "range": [30, 140],
          "component": "Examination",
          "cycles": ["2015-2016", "2017-2018", "2019-2020"],
          "aliases": ["diastolic", "dbp"]
        }
