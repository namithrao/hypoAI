          "data_type": "categorical",
          "values": {
            "1": "Yes",
            "2": "No",
            "7": "Refused",
            "9": "Don't know"
          },
          "component": "Questionnaire",
          "cycles": ["2015-2016", "2017-2018", "2019-2020"],
          "aliases": ["alcohol_history"]
